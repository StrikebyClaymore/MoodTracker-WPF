<Window x:Class="MoodTracker.Windows.DBTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoodTracker.Windows"
        mc:Ignorable="d"
        Title="Window1" Height="680" Width="640">
    <Grid>
        <DatePicker x:Name="DatePicker" HorizontalAlignment="Left" VerticalAlignment="Top" Width="240" Height="24" Margin="16,32,0,0"/>
        <TextBlock VerticalAlignment="Top" Margin="318,16,0,0" Text="Moods"></TextBlock>
        <ComboBox x:Name="MoodBox" VerticalAlignment="Top" Margin="32,32,0,0" Width="128" Height="24">
            <ComboBoxItem>Rad</ComboBoxItem>
            <ComboBoxItem>Good</ComboBoxItem>
            <ComboBoxItem>Neutral</ComboBoxItem>
            <ComboBoxItem>Bad</ComboBoxItem>
            <ComboBoxItem>Awful</ComboBoxItem>
        </ComboBox>
        <TextBox x:Name="NoteText" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="16,16,0,0" Width="240" Height="528" FontSize="16"/>
        <Button VerticalAlignment="Top" Margin="32,64,0,0" Width="128" Height="24" Content="Accept" Click="Button_Click"/>
        <Button VerticalAlignment="Top" Margin="272,96,240,0" Width="128" Height="24" Content="Read" Click="ReadButton_Click"/>
    </Grid>
</Window>
